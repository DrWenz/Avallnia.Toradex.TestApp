             xmlns:local="using:Toradex.TestApp"
             x:Class="Toradex.TestApp.App">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <!-- <FluentTheme Mode="Dark"></FluentTheme> -->

        <Style Selector="TextBlock">
            <Setter Property="FontFamily"
                    Value="avares://Toradex.TestApp/Assets/ProximaNova-Regular.otf#Proxima Nova" />
        </Style>
    </Application.Styles>
</Application>